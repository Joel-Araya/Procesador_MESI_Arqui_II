; PE2 looped dot partial
MOVI R4, 256       ; base A offset (8*8)
MOVI R5, 768      ; base B offset
MOVI R6, 32
MOVI R7, 4
MOVI R0, 0
LOOP2:
LOADR R1, R4
LOADR R2, R5
FMUL R3, R1, R2
FADD R0, R0, R3
ADDI R4, 32
ADDI R5, 32
DEC R7
JNZ LOOP2
MOVI R1, 1120      ; resultado parcial PE2
STORER R0, R1
HALT
