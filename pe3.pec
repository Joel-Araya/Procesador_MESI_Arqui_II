; PE3 looped dot partial
MOVI R4, 384       ; base A offset (12*8)
MOVI R5, 896      ; base B offset
MOVI R6, 32
MOVI R7, 4
MOVI R0, 0
LOOP3:
LOADR R1, R4
LOADR R2, R5
FMUL R3, R1, R2
FADD R0, R0, R3
ADDI R4, 32
ADDI R5, 32
DEC R7
JNZ LOOP3
MOVI R1, 1120      ; resultado parcial PE3
STORER R0, R1
HALT
